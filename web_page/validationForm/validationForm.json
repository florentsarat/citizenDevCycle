      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.customercompensation && $data.customercompensation_claim ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.customercompensation.claim = $data.customercompensation_claim;",
        "\treturn {",
        "\t\t//map customercompensation variable to expected task contract input",
        "\t\tcustomercompensationInput: {",
        "\t\t\tcompensationValidation: $data.customercompensation.compensationValidation !== undefined ? $data.customercompensation.compensationValidation : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
